<div id="searchpanel">
<% form_for @search do |f| %>
  <%= f.label :first_name_or_last_name_like, "Name" %>
  <%= f.text_field :first_name_or_last_name_like, :maxlength => 25, :size => 20 %>
  <%= f.submit "Filter", :class => "searchbutton" %>
<% end %>
  </div>
