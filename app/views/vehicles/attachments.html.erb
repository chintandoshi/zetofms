  <%= link_to 'Back', @vehicle, :class => "backbutton", :style => "float:right;"  %> <br/>

  <h2>Attachments for Vehicle <%=h @vehicle.registration_number %> :</h2>

  <%=  render :partial => "shared/attachments", :object => @vehicle.attachment_boxes  %>

  <p>
      <% form_for [@vehicle, AttachmentBox.new], :url => add_attachment_vehicle_path(@vehicle),:html => { :multipart => true } do |form| %>
        <%= error_messages_for :attachment %>
        <%= form.file_field :uploadedfile %> <br/><br/>
        <%= form.submit "Upload", :class => "attachmentsbutton" %>
      <% end %>
  </p>   
