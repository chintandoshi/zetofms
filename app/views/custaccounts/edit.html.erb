<% render :partial => '/shared/sidebar/contacts' %>

<div id="controls">
    <%= link_to 'Cancel', @custaccount, :class => "cancel" %>
</div>

<h1>Editing Customer Account information</h1>

<% form_for(@custaccount) do |f| %>
  <%= f.error_messages %>

  <%= render :partial => "form", :object => f, :locals => { :mode => "edit"}%>

  <%= f.submit 'Update Account Information', :class => "button" %>
  <%= link_to 'Cancel', @custaccount, :method => 'get' %>
<% end %>