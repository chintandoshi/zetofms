<h1>Welcome to ZETOCRM</h1>

<% form_for @user_session do |f| %>
  <%= f.error_messages %>
  <%= f.label :login %><br />
  <%= f.text_field :login %><br />
  <br />
  <%= f.label :password %><br />
  <%= f.password_field :password %><br />
  <br />
  <%= f.submit "Login", :class => "savebutton" %>
<% end %>

  <p>
    <%= link_to "New User Registration", new_user_url  %>
  </p>
<br/>
